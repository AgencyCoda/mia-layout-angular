{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "d557f33e-8a84-4344-a3c6-71bb0c75d052"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@agencycoda/mia-layout": {
      "projectType": "library",
      "root": "projects/agencycoda/mia-layout",
      "sourceRoot": "projects/agencycoda/mia-layout/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/agencycoda/mia-layout/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/agencycoda/mia-layout/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/agencycoda/mia-layout/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/agencycoda/mia-layout/src/test.ts",
            "tsConfig": "projects/agencycoda/mia-layout/tsconfig.spec.json",
            "karmaConfig": "projects/agencycoda/mia-layout/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "@agencycoda/mia-layout"
}
